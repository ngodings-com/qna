    "deploy-prod:firestore": "firebase deploy --only firestore --token $FIREBASE_TOKEN --non-interactive --force",
    "deploy-prod:storage": "firebase deploy --only storage --token $FIREBASE_TOKEN --non-interactive --force",
    "deploy-prod:hosting": "firebase deploy --only hosting:ngodings-qna --token $FIREBASE_TOKEN --non-interactive",
    "build:prod": "node --max_old_space_size=15000 ./node_modules/@angular/cli/bin/ng build --configuration=production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.0.4",
