    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "use:prod": "firebase use --token $FIREBASE_TOKEN mti-group-96",
